setcps(180/60/4)
do
  d1 $ stack [s "808bd:3" # gain "1.2",s "hh:4*8" # gain "0.9"]
  d2 $ every 3 rev $ n "c3 e3 a2 b2" # s "bass3:7"
  d3 $ n "[c4,e4,a3]" # s "superstatic" #accelerate "1.1"
  d4 $ s "808sd!4" # room "1.1" # gain "0.8"
  d5 $ n "[c3,g3]" # s "superpiano" #delay "0.8" #sustain "1.1"
  d6 $ octave 3 # s "superhex" # room "1.3"

d1 silence
hush
